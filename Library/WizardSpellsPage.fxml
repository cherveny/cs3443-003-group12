            <Tab text="Acid Splash">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button layoutX="672.0" layoutY="524.0" mnemonicParsing="false" onAction="#AcidSplashConfirm" text="Confirm" />
